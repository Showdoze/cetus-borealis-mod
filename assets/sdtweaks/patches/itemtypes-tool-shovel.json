    },

/////////////////shape by type (vanilla is not bad, but still not as compact)

    {
        "op": "replace",
        "path": "/shapeByType",
        "value": { 
            "@shovel-(chert|granite|andesite|basalt|peridotite|flint|obsidian)": { "base": "game:item/tool/shovel-stone" },
            "shovel-*": { "base": "game:item/tool/shovel-copper" }
        },
        "file": "game:itemtypes/tool/shovel"
    },

////////////////////textures by type (just a nicer reorganization)

    {
        "op": "replace",
        "path": "/texturesByType",
        "value": { 
            "@shovel-(chert|granite|andesite|basalt|peridotite|obsidian)": { "material": { "base": "game:block/stone/rock/{material}1" } },
            "shovel-flint": { "material": { "base": "game:item/tool/material/{material}" } },
            "shovel-*": { "material": { "base": "game:block/metal/ingot/{material}" } }
        },
        "file": "game:itemtypes/tool/shovel"
    },

